{
  "require": {
    "ext-dom": "*",
    "ext-zlib": "*",
    "bugo/smf-compat": "*",
    "league/event": "^3.0",
    "nette/utils": "^4.0",
    "spatie/array-to-xml": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Bugo\\Optimus\\": ""
    },
    "exclude-from-classmap": [
      "Addons"
    ]
  },
  "config": {
    "vendor-dir": "Libs",
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.1"
    }
  }
}
